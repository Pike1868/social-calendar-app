{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userGoogleRegistration.json",
    "type": "object",
    "properties": {
      "google_id": {
        "type": "string",
        "minLength": 6
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": ["google_id", "email"]
  }
  