{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 5
    },
    "first_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "last_name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "time_zone":{
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["password", "first_name", "last_name", "email", "time_zone"]
}

  